{
  "name": "theidbrewing.github.io",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "lint-css": "wp-scripts lint-style './src/**/*.scss'",
    "lint-css:fix": "npm run lint-css -- --fix",
    "sass": "npx sass --no-source-map src/scss/style.scss:style.css",
    "postcss:autoprefix": "npx postcss style.css --use autoprefixer --no-map",
    "postcss:svginline": "npx postcss style.css --use postcss-inline-svg --no-map",
    "postcss:all": "npx postcss style.css --use autoprefixer postcss-inline-svg --no-map",
    "build:css": "run-s sass postcss:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/theidbrewing/theidbrewing.github.io.git"
  },
  "author": "theidbrewing",
  "license": "GPL-2.0-or-later",
  "bugs": {
    "url": "https://github.com/theidbrewing/theidbrewing.github.io/issues"
  },
  "devDependencies": {
    "@wordpress/scripts": "^18.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^8.3.1",
    "postcss-inline-svg": "^5.0.0"
  }
}
