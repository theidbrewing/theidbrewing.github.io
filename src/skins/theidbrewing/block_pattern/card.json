	"content": "<!-- wp:columns {\"align\":\"wide\"} -->\n<div class=\"wp-block-columns alignwide\"><!-- wp:column {\"className\":\"is-style-card\"} -->\n<div class=\"wp-block-column is-style-card\"><!-- wp:image {\"sizeSlug\":\"large\",\"linkDestination\":\"none\"} -->\n<figure class=\"wp-block-image size-large\"><img src=\"https://plchldr.co/i/392x392?bg=cccccc\" alt=\"\"/></figure>\n<!-- /wp:image -->\n\n<!-- wp:group {\"style\":{\"spacing\":{\"padding\":{\"right\":\"32px\",\"left\":\"32px\",\"bottom\":\"48px\"}}}} -->\n<div class=\"wp-block-group\" style=\"padding-right:32px;padding-bottom:48px;padding-left:32px\"><!-- wp:paragraph {\"align\":\"center\",\"className\":\"is-style-subtitle\"} -->\n<p class=\"has-text-align-center is-style-subtitle\">0001</p>\n<!-- /wp:paragraph -->\n\n<!-- wp:heading {\"textAlign\":\"center\",\"level\":3,\"style\":{\"typography\":{\"lineHeight\":\"1\"}}} -->\n<h3 class=\"has-text-align-center\" style=\"line-height:1\">IPA</h3>\n<!-- /wp:heading -->\n\n<!-- wp:paragraph -->\n<p>フルーティだけどしっかり苦い。自慢のIPAです。</p>\n<!-- /wp:paragraph -->\n\n<!-- wp:table -->\n<figure class=\"wp-block-table\"><table><tbody><tr><td>ALC</td><td>6.5%</td></tr><tr><td>IBU</td><td>40</td></tr><tr><td>香り</td><td>★★★</td></tr><tr><td>苦さ</td><td>★★★</td></tr></tbody></table></figure>\n<!-- /wp:table --></div>\n<!-- /wp:group --></div>\n<!-- /wp:column -->\n\n<!-- wp:column {\"className\":\"is-style-card\"} -->\n<div class=\"wp-block-column is-style-card\"><!-- wp:image {\"sizeSlug\":\"large\",\"linkDestination\":\"none\"} -->\n<figure class=\"wp-block-image size-large\"><img src=\"https://plchldr.co/i/392x392?bg=cccccc\" alt=\"\"/></figure>\n<!-- /wp:image -->\n\n<!-- wp:group {\"style\":{\"spacing\":{\"padding\":{\"right\":\"32px\",\"left\":\"32px\",\"bottom\":\"48px\"}}}} -->\n<div class=\"wp-block-group\" style=\"padding-right:32px;padding-bottom:48px;padding-left:32px\"><!-- wp:paragraph {\"align\":\"center\",\"className\":\"is-style-subtitle\"} -->\n<p class=\"has-text-align-center is-style-subtitle\">0002</p>\n<!-- /wp:paragraph -->\n\n<!-- wp:heading {\"textAlign\":\"center\",\"level\":3,\"style\":{\"typography\":{\"lineHeight\":\"1\"}},\"textColor\":\"white\"} -->\n<h3 class=\"has-text-align-center has-white-color has-text-color\" style=\"line-height:1\">DIPA</h3>\n<!-- /wp:heading -->\n\n<!-- wp:paragraph {\"align\":\"left\",\"textColor\":\"white\"} -->\n<p class=\"has-text-align-left has-white-color has-text-color\">ガツンと苦い。でもフルーティに。力強いDIPAになりました。</p>\n<!-- /wp:paragraph -->\n\n<!-- wp:table {\"textColor\":\"base\"} -->\n<figure class=\"wp-block-table\"><table class=\"has-base-color has-text-color\"><tbody><tr><td>ALC</td><td>8.5%</td></tr><tr><td>IBU</td><td>56</td></tr><tr><td>香り</td><td>★★★</td></tr><tr><td>苦さ</td><td>★★★</td></tr></tbody></table></figure>\n<!-- /wp:table --></div>\n<!-- /wp:group --></div>\n<!-- /wp:column -->\n\n<!-- wp:column {\"className\":\"is-style-card\"} -->\n<div class=\"wp-block-column is-style-card\"><!-- wp:image {\"sizeSlug\":\"large\",\"linkDestination\":\"none\"} -->\n<figure class=\"wp-block-image size-large\"><img src=\"https://plchldr.co/i/392x392?bg=cccccc\" alt=\"\"/></figure>\n<!-- /wp:image -->\n\n<!-- wp:group {\"style\":{\"spacing\":{\"padding\":{\"right\":\"32px\",\"left\":\"32px\",\"bottom\":\"48px\"}}}} -->\n<div class=\"wp-block-group\" style=\"padding-right:32px;padding-bottom:48px;padding-left:32px\"><!-- wp:paragraph {\"align\":\"center\",\"className\":\"is-style-subtitle\"} -->\n<p class=\"has-text-align-center is-style-subtitle\">0003</p>\n<!-- /wp:paragraph -->\n\n<!-- wp:heading {\"textAlign\":\"center\",\"level\":3,\"style\":{\"typography\":{\"lineHeight\":\"1\"}},\"textColor\":\"white\"} -->\n<h3 class=\"has-text-align-center has-white-color has-text-color\" style=\"line-height:1\">PALE ALE</h3>\n<!-- /wp:heading -->\n\n<!-- wp:paragraph {\"align\":\"left\",\"textColor\":\"white\"} -->\n<p class=\"has-text-align-left has-white-color has-text-color\">淡い色。でも濃いエール。ホップの香りが強いアメリカンスタイルです。</p>\n<!-- /wp:paragraph -->\n\n<!-- wp:table {\"textColor\":\"base\"} -->\n<figure class=\"wp-block-table\"><table class=\"has-base-color has-text-color\"><tbody><tr><td>ALC</td><td>5.5%</td></tr><tr><td>IBU</td><td>30</td></tr><tr><td>香り</td><td>☆★★</td></tr><tr><td>苦さ</td><td>☆★★</td></tr></tbody></table></figure>\n<!-- /wp:table --></div>\n<!-- /wp:group --></div>\n<!-- /wp:column --></div>\n<!-- /wp:columns -->"
